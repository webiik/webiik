{
  "compilerOptions": {
    /* https://www.typescriptlang.org/tsconfig */
    /* Project Options */
    "allowJs": true,
    "jsx": "react",
    "module": "es6",
    "outDir": "../../../../public/assets/js/",
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    /* Strict Checks */
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    /* Module Resolution */
    "esModuleInterop": true,
    "moduleResolution": "node",
  }
}